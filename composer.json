{
    "name": "iamrohitpal/team-tasker",
    "description": "Laravel Todo app, team-tasker",
    "type": "project",
    "keywords": ["laravel", "todo", "team-tasker"],
    "homepage": "http://localhost/",
    "license": "MIT",
    "authors": [
        {
            "name": "Rohit Pal",
            "email": "iamrohitpalg@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate"
        ],
        "post-update-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Package\\ServiceProvider"
            ]
        }
    }
}
